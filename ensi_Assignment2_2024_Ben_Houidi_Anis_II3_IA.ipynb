        "        delay of 1 sec for ease of understanding\n",
        "    \"\"\"\n",
        "    os.system('clear')\n",
        "    q_world.print_world(action, step,q_agent.is_explore)\n",
        "    q_agent.print_q_table()\n",
        "    if done:\n",
        "        print(\"-------EPISODE DONE--------\")\n",
        "        delay *= 2\n",
        "    time.sleep(delay)"
      ]
